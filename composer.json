{
    "name": "peanutsquare/mediainfo-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for using php-mediainfo as service",
    "license": "MIT",
    "authors": [
        {
            "name": "mhor",
            "email": "maxime.horcholle@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^2.3 || ^3.0",
        "peanutsquare/php-mediainfo" : "dev-master"
    },
    "repositories" : [{
        "type" : "vcs",
        "url" : "https://github.com/PeanutSquareLLP/MhorMediaInfoBundle.git"
    }],
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0"
    },
    "autoload": {
        "psr-4": { "Mhor\\MediaInfoBundle\\" : "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.x-dev"
        }
    }
}
